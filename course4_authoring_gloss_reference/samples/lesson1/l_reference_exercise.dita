<reference id="reference_jgf_3df_vt">
  <title>Analyzing existing information products</title>
  <refbody>
    <section>The <cmdname>addEntry</cmdname> command is used to enter a new documentation release
      into the content analysis database.</section>
    <refsyn>
      <synph>-addEntry {nLanguage | nVersion | nOutput}</synph>
    </refsyn>
    <properties id="properties_qxt_3df_vt">
      <prophead>
        <proptypehd>Required?</proptypehd>
        <propvaluehd>Pages</propvaluehd>
        <propdeschd>Delivery format</propdeschd>
      </prophead>
      <property>
        <proptype>Yes</proptype>
        <propvalue>nLanguage</propvalue>
        <propdesc>The language displayed by default</propdesc>
      </property>
      <property>
        <proptype></proptype>
        <propvalue>nVersion</propvalue>
        <propdesc>The version of the content displayed by default</propdesc>
      </property>
      <property>
        <proptype></proptype>
        <propvalue>nOutput</propvalue>
        <propdesc>The required delivery format</propdesc>
      </property>
    </properties>
  </refbody>
</reference>
