                                        <fo:block-container background-color="rgb(238 , 238 , 238)" font-family="monospace" font-size="smaller" space-after="0.5em" margin-left="0.5em" margin-right="0" space-before="0.5em" max-height="600px" padding-bottom="0.5em" padding-left="0.5em" padding-right="0.5em" padding-top="0.5em" linefeed-treatment="preserve" white-space-collapse="false" white-space-treatment="preserve" wrap-option="wrap"><fo:block end-indent="0" start-indent="0" role="Code">&lt;simpletable&gt;
   &lt;sthead&gt;
