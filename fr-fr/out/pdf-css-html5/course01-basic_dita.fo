                                <fo:block id="assessment_bkn_skb_wr2" font-size="0" keep-with-next="always"/><fo:block role="Sect" id="unique_37">
                                    <fo:block-container font-size="1.4em" font-weight="300" space-after="0.4em" margin-left="0" margin-right="0" space-before="1.3em" page-break-after="avoid" id="ariaid-title37"><fo:block end-indent="0" start-indent="0" role="H3"/></fo:block-container>
                                    
                                    <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
                                        <fo:block-container space-after="1em" space-before="1em"><fo:block end-indent="0" start-indent="0" role="Sect">
                                            <fo:block id="md12" font-size="0" keep-with-next="always"/><fo:block id="unique_37_Connect_42_md12">
                                                <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">Les métadonnées sont des
                                                  informations sur le contenu.</fo:block></fo:block-container>
                                                <fo:list-block relative-align="baseline" provisional-label-separation="3pt" provisional-distance-between-starts="2em" space-after="1.33em" space-before="1.33em">
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol">•</fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0">
                                                  <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">Vrai</fo:block></fo:block-container>
                                                  </fo:block></fo:block-container></fo:block></fo:list-item-body></fo:list-item>
                                                  <fo:list-item><fo:list-item-label end-indent="label-end()"><fo:block text-align="end"><fo:inline font-family="Times New Roman, Segoe UI Symbol">•</fo:inline></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block-container><fo:block start-indent="0" end-indent="0">
                                                  <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">Faux</fo:block></fo:block-container>
                                                  </fo:block></fo:block-container></fo:block></fo:list-item-body></fo:list-item>
                                                </fo:list-block>
                                                <fo:block-container space-after="0.5em" margin-left="0" margin-right="0" space-before="0.5em"><fo:block end-indent="0" start-indent="0">Les métadonnées sont
                                                  souvent décrites comme étant "de l'information sur
                                                  l'information". Elles fournissent un contexte
                                                  supplémentaire à votre contenu.</fo:block></fo:block-container>
                                            </fo:block>
                                        </fo:block></fo:block-container>
                                    </fo:block></fo:block-container>
                                </fo:block>
                                <fo:block id="assessment_bkn_skb_wr3" font-size="0" keep-with-next="always"/><fo:block role="Sect" id="unique_38">
                                    <fo:block-container font-size="1.4em" font-weight="300" space-after="0.4em" margin-left="0" margin-right="0" space-before="1.3em" page-break-after="avoid" id="ariaid-title38"><fo:block end-indent="0" start-indent="0" role="H3"/></fo:block-container>
                                    
                                    <fo:block-container space-after="0" margin-left="0" margin-right="0" space-before="0"><fo:block end-indent="0" start-indent="0">
